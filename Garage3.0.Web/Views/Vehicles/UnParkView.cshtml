@model Garage3._0.Core.Vehicle
@inject IVehicleTypeSelectListService service
@{
    ViewData["Title"] = "Unpark your vehicle";
}
<h1>@ViewData["Title"]</h1>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-controller="Vehicles" asp-action="Delete">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="RegistrationNumber" class="control-label"></label>
                <input asp-for="RegistrationNumber" class="form-control" />
                <span asp-validation-for="RegistrationNumber" class="text-danger"></span>
             <div class="form-group">
                <input type="submit" value="Unpark your vehicle" class="btn btn-primary" />
                </div>
            </div>
        </form>
    </div>
</div>